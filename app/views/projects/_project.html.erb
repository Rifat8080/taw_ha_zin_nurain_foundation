<div id="<%= dom_id project %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Name:</strong>
    <%= project.name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Categories:</strong>
    <%= project.categories %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Description:</strong>
    <%= project.description %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Project status active:</strong>
    <%= project.project_status_active? ? "Yes" : "No" %>
  </div>
</div>
